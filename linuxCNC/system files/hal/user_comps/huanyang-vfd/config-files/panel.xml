<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
	<labelframe text="DRO">
	<font>("Helvetica",12)</font>
	<table>
		<tablerow/>
    			<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
	    		</label>
		<tablerow/>
		    	<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
	    		</label>
			<label>
    				<text>"X"</text>
				<font>("courier 10 pitch",30)</font>
				<width>2</width>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
	    		</label>
			<number>
				<halpin>"Xdisplay"</halpin>
				<format>"+4.2f"</format>
				<font>("courier 10 pitch",30)</font>
				<width>8</width>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
			</number>
			<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
    				<width>1</width>
	    		</label>
