			<button>
				<text>"Zero"</text>
				<halpin>"Xzerobutton"</halpin>
			</button>
			<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
	    		</label>
		<tablerow/>
			<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
	    		</label>
			<label>
    				<text>"Y"</text>
				<font>("courier 10 pitch",30)</font>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
	    		</label>
			<number>
				<halpin>"Ydisplay"</halpin>
				<format>"+4.2f"</format>
				<font>("courier 10 pitch",30)</font>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
			</number>
			<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
    				<width>1</width>
	    		</label>
			<tablesticky sticky="nsew" />
			<button>
				<text>"Zero"</text>
				<halpin>"Yzerobutton"</halpin>
			</button>
		<tablerow/>
		    	<label>
    				<text>""</text>
    				<font>("Helvetica",2)</font>
	    		</label>
			<label>
    				<text>"Z"</text>
				<font>("courier 10 pitch",30)</font>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
	    		</label>
			<number>
				<halpin>"Zdisplay"</halpin>
				<format>"+4.2f"</format>
				<font>("courier 10 pitch",30)</font>
				<foreground>"#7FFF00"</foreground>
				<background>"#000000"</background>
